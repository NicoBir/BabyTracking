<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.babytracking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BabyActivity"
            android:label="Baby">
        </activity>
        <activity
            android:name=".BreastActivity"
            android:label="Breast">
        </activity>
        <activity
            android:name=".DiaperActivity"
            android:label="Diaper">
        </activity>
        <activity
            android:name=".MilestoneActivity"
            android:label="Milestone">
        </activity>
        <activity
            android:name=".SleepActivity"
            android:label="sleep">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>